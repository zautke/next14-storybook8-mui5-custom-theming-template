	"scripts": {
		"biome": "yarn dlx @biomejs/biome check --apply src/",
		"build-storybook": "storybook build",
		"build:analyze": "npm run build && source-map-explorer .next/static/**/*.js",
		"build:dev": "next build",
		"build:prod": "NODE_ENV=production tsc -p tsconfig.production.json && next build --production",
		"build": "STANDALONE=true next build",
		"dev": "next dev",
		"image": "docker buildx build . -f nextjs.Dockerfile -t braisenly/lab:$(echo $(cat package.json | jq -r '.version')) --platform linux/amd64 && docker tag braisenly/lab:$(echo $(cat package.json | jq -r '.version')) braisenly/lab:latest",
		"image:arm64": "docker build . -f nextjs.Dockerfile -t braisenly/lab:arm64 --platform linux/arm64",
		"lint": "next lint",
		"release": "release-it",
		"start-server": "tsx server/ws-server.ts",
		"start": "next start -p ${PORT:=3000}",
		"standalone": "node .next/standalone/server.js",
		"storybook": "storybook dev -p 6006",
		"test:ci": "jest --runInBand --no-cache --coverage --verbose",
		"test:watch": "jest --watch",
		"test": "jest --coverage --verbose",
		"ws": "tsx server/ws2.ts"
	},
